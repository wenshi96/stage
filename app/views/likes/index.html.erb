<div>
  <div>
    <h1>
      My likes
    </h1>
  </div>
</div>

<hr>


<hr>
<div>
  <div>
    <% if @current_user.likes.count != 0%>
    <table border="1">
      <tr>
        <th>
          Photo Owner
        </th>

        <th>
          Created at
        </th>

        <th>
        </th>
      </tr>

      <% @list_of_likes.each do |a_like| %>
      <tr>

        <td>
          <%= a_like.photo.owner.username %>
        </td>

        <td>
          <%= time_ago_in_words(a_like.created_at) %> ago
        </td>

        <td>
          <a href="photos/<%= a_like.photo_id %>">          
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
    <%else%>
    <div>You haven't liked any post yet.</div>
    <%end%>
  </div>
</div>

<hr>
